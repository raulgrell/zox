(class Fact
    (init [p_a p_b]
        (set :a p_a)
        (set :b p_b)
        (print ))
    
    (defn gg [] 
        (var s (+ :a :b))
        (print s)
        (set ::a ::b)
        (set ::b ::s)))

(var fk (clock))
(var f (Fact 0, 1))
(var j 0)

(while (< j 200)
    (:gg f)
    (set j (+ j 1)))

(var ft (- (clock) fk))
(print ft)
